@using (Script.Foot())
{


    <script type="text/javascript">
        history.pushState(null, null, 'GetStarted');
        window.addEventListener('popstate', function (event) {
            history.pushState(null, null, 'GetStarted');
            NextPage();
        });
    </script>
    <script type="text/javascript">
        $('.textarea').wysihtml5();
        $(".flip-buttons").click(function () {
            var $elem = $("#view-front");

            if (($elem[0].attributes.style.value.indexOf("rotateY(0deg)")) > -1) {
                setTimeout(function () {
                    $(".flip-button-front").css("background-color", "#6c7478");
                    $(".flip-button-front").css("color", "white");
                    $(".flip-button-back").css("background-color", "white");
                    $(".flip-button-back").css("color", "#6c7478");
                }, 180);
            } else
                if (($elem[0].attributes.style.value.indexOf("rotateY(180deg)")) > -1) {
                    setTimeout(function () {
                        $(".flip-button-back").css("background-color", "#6c7478");
                        $(".flip-button-back").css("color", "white");
                        $(".flip-button-front").css("background-color", "white");
                        $(".flip-button-front").css("color", "#6c7478");
                    }, 180);

                }
        });

        function sendData() {
            var countTees = document.getElementById('trackBarValue').value;
            var profit = document.getElementById('total_profit').innerHTML;
            var itemOptions = document.getElementById('item-options-dropdown').value;
            var product = document.getElementById('product').value;
            var campaignTitle = document.getElementById('name').value;
            var description = document.getElementById('campaign_description_text').value;
            var campaignLength = document.getElementById('length').value;
            var alias = document.getElementById('url').value;
            var backSideByDefault = document.getElementById('backdefaultside').checked;
            $.ajax({
                async: false,
                cache: false,
                type: "POST",
                url: "@(Url.Action("LaunchCampaign", "Wizard"))",
                data: addAntiForgeryToken({
                    "ProductCountGoal": countTees, "profit": profit, "itemOptions": itemOptions, "product": product,

                    "CampaignTitle": campaignTitle, "Description": description, "CampaignLength": campaignLength, "Alias": alias,
                    'BackSideByDefault': backSideByDefault
                }),
                success: function (data, status, resp) {
                    if (resp.status === 200) {
                        window.location.href = '/' + alias;
                    } 
                },
                error: function (data, status, resp) {
                    alert('Status: ' + data.status + '. Error message: ' + data.statusText);
                }
            });

        }

        function addAntiForgeryToken(data) {
            data.__RequestVerificationToken = "@Html.AntiForgeryTokenValueOrchard()";
            return data;
        };

        var isFront = true
        $(function ($) {
            $("#card").flip({
                trigger: 'manual'
            }).click(function () {
                isFront = !isFront;
                console.log(isFront);
                $("#card").flip(!isFront);
            });
        });

        var isFront3 = true
        $(function ($) {
            $("#card3").flip({
                trigger: 'manual'
            }).click(function () {
                isFront3 = !isFront3;
                console.log(isFront3);
                $("#card3").flip(!isFront3);
            });
        });



    </script>
}




