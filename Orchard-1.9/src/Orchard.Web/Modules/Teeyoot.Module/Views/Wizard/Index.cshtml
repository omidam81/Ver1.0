@using (Script.Head())
{
    <script>
        window.fbAsyncInit = function() {
            FB.init({
                appId: '@Model.FacebookClientId',
                xfbml: true,
                version: 'v2.4',
                status: true
            });
        };

        (function(d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) {
                return;
            }
            js = d.createElement(s);
            js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    </script>

    <script>
        window.GoogleClientId = '@Model.GoogleClientId';
        window.GoogleApiKey = '@Model.GoogleApiKey';
        window.GoogleScopes = 'https://www.googleapis.com/auth/userinfo.email';

        function handleAuthResult(authResult) {
            window.GoogleAccessToken = authResult.access_token;
        }

        function checkAuth() {
            gapi.auth.authorize({
                client_id: window.GoogleClientId,
                scope: window.GoogleScopes,
                immediate: true
            }, handleAuthResult);
        };

        function handleClientLoad() {
            gapi.client.setApiKey(window.GoogleApiKey);
            window.setTimeout(checkAuth, 1);
        };
    </script>

    <script src="https://apis.google.com/js/client.js?onload=handleClientLoad"></script>
}

