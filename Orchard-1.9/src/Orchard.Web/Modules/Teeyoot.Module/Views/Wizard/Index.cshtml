        function refreshToken() {
            $.ajax({
                async: false,
                type: 'GET',
                url: '/RefreshToken',
                success: function(data) {
                    $('#anti_forgery_token').val(data);
                }
            });
        };

        function googleWizardLogin(token) {
            $.ajax({
                type: 'POST',
                url: '/WizardGoogleAuth',
                data: addAntiForgeryToken({
                    token: token
                }),
                success: function(data) {
                    if (data.Success === true) {
                        refreshToken();
                        closeAuthModal();
                        sendData();
                        return;
                    }
                    showAuthModal(data.Message);
                }
            });
        };

        function authCallback(authResult) {
            var token = authResult.access_token;
            if (token !== undefined) {
                googleWizardLogin(token);
            }
        };

        function handleAuthClick(event) {
            gapi.auth.authorize({
                client_id: window.GoogleClientId,
                scope: window.GoogleScopes,
                immediate: false
            }, authCallback);
            return false;
        };

        function fbWizardLogin(token) {
            $.ajax({
                type: 'POST',
                url: '/WizardFacebookAuth',
                data: addAntiForgeryToken({
                    token: token
                }),
                success: function(data) {
                    if (data.Success === true) {
                        refreshToken();
                        closeAuthModal();
                        sendData();
                        return;
                    }
                    showAuthModal(data.Message);
                }
            });
        };

        function fbLoginCallback(response) {
            if (response.status === 'connected') {
                var token = response.authResponse.accessToken;
                fbWizardLogin(token);
            }
        };

        function fbLogin() {
            FB.login(function(response) {
                fbLoginCallback(response);
            }, { scope: 'email' });
        };

        function fbGetLoginStatusCallback(response) {
            if (response.status === 'connected') {
                var token = response.authResponse.accessToken;
                fbWizardLogin(token);
                return;
            }
            fbLogin();
        };

        function fbGetLoginStatus() {
            FB.getLoginStatus(function(response) {
                fbGetLoginStatusCallback(response);
            });
        };

        function showAuthModal(socialLoginFailMessage) {
            if (socialLoginFailMessage !== undefined) {
                $('#social_login_failed').text(socialLoginFailMessage);
                $('#social_login_failed').show();
            } else {
                $('#social_login_failed').hide();
            }

            $('#overlay').fadeIn(400, function() {
                $('.auth-window').hide();
                $('#login_selection').show();
                $('#modal_auth')
                    .css('display', 'block')
                    .animate({ opacity: 1, top: '50%' }, 200);
            });
        };

        function closeAuthModal() {
            $('#modal_auth').animate({ opacity: 0, top: '45%' }, 200,
                function() {
                    $(this).css('display', 'none');
                    $('#overlay').fadeOut(400);
                }
            );
        };

        $(document).ready(function() {
            $('#overlay, .close').click(function() {
                closeAuthModal();
            });

            $('.login-with-teeyoot').click(function() {
                $('.auth-window').hide();
                $('#login_teeyoot').show();
            });

            $('.create-new-account').click(function() {
                $('.auth-window').hide();
                $('#create_account').show();
            });

            $('#login_form').submit(function() {
                var busy = $(this).data('busy');
                if (busy === true) {
                    return false;
                }

                var email = $('#login_teeyoot .email').val();
                var password = $('#login_teeyoot .password').val();

                var wizardLogOnJsonRequest = {
                    Email: email,
                    Password: password
                };

                $.ajax({
                    type: 'POST',
                    url: '/WizardLogOn',
                    data: addAntiForgeryToken(wizardLogOnJsonRequest),
                    beforeSend: function() {
                        $('#login_form').data('busy', true);
                        $('#login_busy').show();
                    },
                    success: function(data) {
                        if (data.Success === true) {
                            refreshToken();
                            closeAuthModal();
                            sendData();
                            return;
                        }

                        $('#login_validation_summary').text(data.Message);
                        $('#login_validation_summary').show();
                    },
                    complete: function() {
                        $('#login_form').data('busy', false);
                        $('#login_busy').hide();
                    }
                });

                return true;
            });





            var result = new Array();
            function res(request, response) {

                $.ajax({
                    async: false,
                    cache: false,
                    type: "GET",
                    url: "@(Url.Action("GetDetailTags", "Wizard"))",
                    data: { "filter": document.getElementById("inputTags").value },
                    success: function(data) {
                        for (var i = 0; i < data.length; i++) {
                            result[i] = data[i];
                        }
                    },
                    error: function (data, status, resp) {
                        alert('Status: ' + data.status + '. Error message: ' + data.statusText);
                    }

                });

            }
            res();

            var rest = [];


            $('#inputTags').selectize({
                plugins: ['remove_button'],
                delimiter: ',',
                persist: false,
                valueField: 'name',
                labelField: 'name',
                searchField: ['name'],
                create: function(input) {
                    return {
                        name: input
                    }
                },
                options: result,
            });



            $('#create_account_form').submit(function() {
                var busy = $(this).data('busy');
                if (busy === true) {
                    return false;
                }

                var email = $('#create_account .email').val();
                var password = $('#create_account .password').val();
                var confirmPassword = $('#create_account .confirm-password').val();
                var name = $('#create_account .name').val();
                var phone = $('#create_account .phone').val();

                var wizardRegisterJsonRequest = {
                    Email: email,
                    Password: password,
                    ConfirmPassword: confirmPassword,
                    Name: name,
                    Phone: phone
                };

                $.ajax({
                    type: 'POST',
                    url: '/WizardRegister',
                    data: addAntiForgeryToken(wizardRegisterJsonRequest),
                    beforeSend: function() {
                        $('#create_account_form').data('busy', true);
                        $('#create_account_busy').show();
                    },
                    success: function(data) {
                        if (data.Success === true) {
                            refreshToken();
                            closeAuthModal();
                            sendData();
                            return;
                        }

                        $('#create_account_validation_summary').text(data.Message);
                        $('#create_account_validation_summary').show();
                    },
                    complete: function() {
                        $('#create_account_form').data('busy', false);
                        $('#create_account_busy').hide();
                    }
                });

                return true;
            });

            $('#login_with_facebook').click(function() {
                fbGetLoginStatus();
            });

            $('#login_with_google').click(function(e) {
                handleAuthClick(e);
            });
        });

        function openTagsAll() {
            document.getElementById("tags").style.display = "inline";
            document.getElementById("openTagsButton").style.display = "none";
        };


        var slider = document.getElementById('trackbar');
        var input = document.getElementById('trackBarValue');

        var maxRange = window.salesTarget;
        var step = parseFloat(maxRange) / parseFloat("5");
        var minSteps = step.toFixed(0);
        var step1 = minSteps;
        var step2 = minSteps * 2;
        var step3 = minSteps * 3;
        var step4 = minSteps * 4;

        noUiSlider.create(slider, {
            start: window.count,
            orientation: 'horizontal',
            step: 1,
            range: {
                'min': 15,
                '50%': [maxRange/2, 1],
                'max': maxRange
            },
            pips: {
                mode: 'values',
                values: [15, step1, step2, step3, step4, maxRange],
                density: 4
            }
        });

        slider.noUiSlider.on('update', function( values, handle ) {
            var goal = parseFloat(values[handle]);
            input.value = goal.toFixed(0);
            onChangeTrackBar();
        });

        input.addEventListener('change', function(){
            slider.noUiSlider.set(this.value);
        });

        // Listen to keydown events on the input field.
        input.addEventListener('keydown', function( e ) {

            // Convert the string to a number.
            var value = Number( slider.noUiSlider.get() ),
                sliderStep = slider.noUiSlider.steps()

            // Select the stepping for the first handle.
            sliderStep = sliderStep[0];

            // 13 is enter,
            // 38 is key up,
            // 40 is key down.
            switch ( e.which ) {
                case 13:
                    slider.noUiSlider.set(this.value);
                    break;
                case 38: slider.noUiSlider.set( value + sliderStep[1] );
                    break;
                case 40: slider.noUiSlider.set( value - sliderStep[0] );
                    break;
            }
        });
    </script>


    <script>
        jQuery.validator.setDefaults({
            debug: true,
            success: "valid"
