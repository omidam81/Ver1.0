        function sendData() {
            var countTees = document.getElementById('trackBarValue').value;
            var profit = document.getElementById('total_profit').innerHTML;
            var itemOptions = document.getElementById('item-options-dropdown').value;
            var product = document.getElementById('product').value;
            var campaignTitle = document.getElementById('name').value;
            var description = document.getElementById('campaign_description_text').value;
            var campaignLength = document.getElementById('length').value;
            var alias = document.getElementById('url').value.trim();
            var backSideByDefault = document.getElementById('backdefaultside').checked;
            var tagsElems = document.querySelectorAll(".item");
            var productMinimumGoal = document.getElementById("minimmumGoal").value;
            var cntFrontColor = window.frontColor;
            var cntBackColor = window.backColor;
            var tags = [];
            for (var i = 0; i < tagsElems.length; i++) {
                //tags.push(tagsElems[i].innerText.substring(0, tagsElems[i].innerText.length - 1));
                tags.push($(tagsElems[i]).text().substring(0, $(tagsElems[i]).text().length - 1));
            }
            //var isForCharity = document.getElementById('is_for_charity').checked;
            $.ajax({
                async: false,
                cache: false,
                type: "POST",
                url: "@(Url.Action("LaunchCampaign", "Wizard"))",
                data: addAntiForgeryToken({
                    "ProductCountGoal": countTees,
                    "CampaignProfit": profit,
                    "itemOptions": itemOptions,
                    "product": product,
                    "Design": JSON.stringify(app.state.design),
                    //'IsForCharity': isForCharity,
                    "CampaignTitle": campaignTitle,
                    "Description": description,
                    "CampaignLength": campaignLength,
                    "Alias": alias,
                    'BackSideByDefault': backSideByDefault,
                    "Tags": tags,
                    "Products": app.state.products,
                    "ProductMinimumGoal" : productMinimumGoal,
                    "CntFrontColor" : cntFrontColor,
                    "CntBackColor" : cntBackColor
                }),
                success: function(data, status, resp) {
                    if (resp.status === 200) {
                        window.location.href = '/' + alias;
                    }
                },
                error: function(data, status, resp) {
                    if (data.status === 403) {
                        showAuthModal();
                        return;
                    }
                    $('#name').removeClass('wizard-error');
                    $('.wysihtml5-sandbox').removeClass('wizard-error');
                    $('#url').removeClass('wizard-error');
                    $('#name_error-text').css('display', 'none');
                    $('#campaign_description_text_error-text').css('display', 'none');
                    $('#url_error-text').css('display', 'none');
                    var errorsData = data.statusText.split('|');
                    for(var i = 0; i < errorsData.length; i++){
                        if(errorsData[i]){
                            var idForData = '#' + errorsData[i];
                            var idForError = '#' + errorsData[i] + '_error-text';
                            if(errorsData[i] == 'campaign_description_text'){
                                idForData = '.wysihtml5-sandbox';
                            }
                            var error = $(idForData);
                            var error_text = $(idForError)[0];
                            error.addClass('wizard-error');
                            i++;
                            $(idForError).css('display', 'block');
                            error_text.innerHTML = errorsData[i];
                        }
                    }
                }
            });
        };

