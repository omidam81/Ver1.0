@using (Script.Foot())
{

    <script type="text/javascript">

        $(".flip-buttons").click(function () {
            var $elem = $("#view-front");

            if (($elem[0].attributes.style.value.indexOf("rotateY(0deg)")) > -1) {
                setTimeout(function () {
                    $(".flip-button-front").css("background-color", "#44474d");
                    $(".flip-button-front").css("color", "white");
                    $(".flip-button-back").css("background-color", "white");
                    $(".flip-button-back").css("color", "#44474d");
                }, 180);
            } else
                if (($elem[0].attributes.style.value.indexOf("rotateY(180deg)")) > -1) {
                    setTimeout(function () {
                        $(".flip-button-back").css("background-color", "#44474d");
                        $(".flip-button-back").css("color", "white");
                        $(".flip-button-front").css("background-color", "white");
                        $(".flip-button-front").css("color", "#44474d");
                    }, 180);

                }
        });


        var isFront = true
        $(function ($) {
            $("#card").flip({
                trigger: 'manual'
            }).click(function () {
                isFront = !isFront;
                console.log(isFront);
                $("#card").flip(!isFront);
            });
        });


    </script>
}


