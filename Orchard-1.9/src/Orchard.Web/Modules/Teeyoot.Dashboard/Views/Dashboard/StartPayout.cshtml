

        <form id="form" class="form-horizontal">
            <div class="modal-title-payout"><h1>@T("Send us a payout request by filling in your payment information")</h1></div>
            <div class="form-group">
                <label class="col-sm-4 control-label">@T("Account number")</label>
                <div class="col-sm-8">
                    <input type="text" id="accountNum"  class="form-control">
                    <label id="lblAccountNum" class="error valid"></label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-4 control-label">@T("Bank Name")</label>
                <div class="col-sm-8">
                    <input type="text" id="bankName" class="form-control">
                    <label id="lblBankName" class="error valid"></label>
                </div>
            </div>
            <div class="form-group">


                <label class="col-sm-4 control-label">@T("Account Holder Name")</label>
                <div class="col-sm-8">
                    <input type="text" id="accHoldName" class="form-control">
                    <label id="lblAccHoldName" class="error valid"></label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-4 control-label">@T("Contact Number ")</label>
                <div class="col-sm-8">
                    <input type="text" id="contNum" maxlength="15" class="form-control" >
                    <label id="lblContNum" class="error valid"></label>
                </div>
            </div>

            <div class="form-group-message form-group">


                <label class="col-sm-4 control-label">@T("Message for the admin")</label>
                <div class="col-sm-8">
                    <textarea style="resize: none;" id="messAdmin" class="form-control"></textarea>
                    <label id="lblMessAdmin" class="error valid"></label>
                </div>
            </div>

            <div class="form-group">
                <div class="col-sm-offset-4 col-sm-8">
                    <a class="btnStartPayout" onclick="sendData()">@T("Submit")</a>
                </div>
            </div>
</form>