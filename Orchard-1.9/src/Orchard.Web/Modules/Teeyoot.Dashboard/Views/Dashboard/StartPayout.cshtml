

        <form class="form-horizontal">
            <div class="form-group">
                <label class="col-sm-4 control-label">Account number</label>
                <div class="col-sm-8">
                    <input type="text" id="accountNum" class="form-control">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-4 control-label">Bank Name</label>
                <div class="col-sm-8">
                    <input type="text" id="bankName" class="form-control">
                </div>
            </div>
            <div class="form-group">


                <label class="col-sm-4 control-label">Account Holder Name</label>
                <div class="col-sm-8">
                    <input type="text" id="accHoldName" class="form-control">
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-4 control-label">Contact Number </label>
                <div class="col-sm-8">
