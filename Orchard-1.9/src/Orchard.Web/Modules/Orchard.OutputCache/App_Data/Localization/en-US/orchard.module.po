msgid "Default Cache Duration"
msgstr "Default Cache Duration"

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Number of seconds that items should be cached on the server before being regenerated."
msgstr "Number of seconds that items should be cached on the server before being regenerated."

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Default Cache Grace Time"
msgstr "Default Cache Grace Time"

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Number of seconds past duration that stale items can be served from cache while regenaration is in progress. Enter 0 to disable grace time by default."
msgstr "Number of seconds past duration that stale items can be served from cache while regenaration is in progress. Enter 0 to disable grace time by default."

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Max Age"
msgstr "Max Age"

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "When defined, a cache-control header with a max-age property will be added. Use this in order to enable kernel cache on IIS."
msgstr "When defined, a cache-control header with a max-age property will be added. Use this in order to enable kernel cache on IIS."

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Vary by Query String Parameters"
msgstr "Vary by Query String Parameters"

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "When defined, using comma separated values, sets caching to vary by the specified query string parameters. Leave empty to vary by all query string parameters."
msgstr "When defined, using comma separated values, sets caching to vary by the specified query string parameters. Leave empty to vary by all query string parameters."

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Vary by Request Headers"
msgstr "Vary by Request Headers"

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "When defined, using comma separated values, sets caching to vary by the specified request headers."
msgstr "When defined, using comma separated values, sets caching to vary by the specified request headers."

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Ignored URLs"
msgstr "Ignored URLs"

#: ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgctxt ~/Modules/Orchard.OutputCache/Views/Admin/Index.cshtml
msgid "Specifies a set of paths relative to tenant root (e.g. '/' or '/About') for which caching should not be done. Enter one path per line."
msgstr "Specifies a set of paths relative to tenant root (e.g. '/' or '/About') for which caching should not be done. Enter one path per line."

